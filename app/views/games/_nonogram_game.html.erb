<%= form_tag [@game_presented, @player], id: 'player-answer', method: :put do %>
  <%= render partial: 'nonograms/interactive', locals: {size: @size, nonogram: @game_presented.nonogram, draw_clues: true} %>
  <%= hidden_field_tag 'type', 'check' %>



  <div  class="button2">
  	<%= submit_tag 'Check Answer' %>
  </div>
  
  <button class="button2" id="give-up" type="button">Give up</button>
<% end %>