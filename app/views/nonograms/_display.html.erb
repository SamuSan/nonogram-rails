<table id="nonogram-display" align="center" >
  <% @game.nonogram.size.times do |row| %>
    <tr>
      <% @game.nonogram.size.times do |column| %>
        <%= content_tag(:td, '', class: presentable_cell_class(row: row, column: column, nonogram: nonogram)) %>
      <% end %>
    </tr>
  <% end %>
</table>